<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Darker background color for the square -->
    <solid android:color="#E6E6E6" /> <!-- Lighter grey background color -->

    <!-- Darker stroke to create a border effect -->
    <stroke android:width="1dp" android:color="#A0A0A0" /> <!-- Darker grey border -->

    <!-- Adjust opacity to make the background slightly darker -->
    <solid android:color="#FFFFFF" /> <!-- Slightly more opaque (darker) white -->
</shape>
